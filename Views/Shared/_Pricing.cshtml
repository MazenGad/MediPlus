@model IEnumerable<Service>
@inject IServiceHelper ServiceHelper

		<!-- Pricing Table -->
<section class="pricing-table section">
	<div class="container">
		<div class="row">
			<div class="col-lg-12">
				<div class="section-title">
					<h2>We Provide You The Best Treatment In Resonable Price</h2>
					<img src="img/section-img.png" alt="#">
					<p>Lorem ipsum dolor sit amet consectetur adipiscing elit praesent aliquet. pretiumts</p>
				</div>
			</div>
		</div>
		<div class="row">
			<!-- Single Table -->
			@{
				int count = 0;
				foreach (var service in Model)
				{
					<div class="col-lg-4 col-md-12 col-12">
						<div class="single-table">
							<!-- Table Head -->
							<div class="table-head">
								<div class="icon">
									<i class="icofont @ServiceHelper.GetServiceIcon(service.Name)"></i> -->
								</div>
								<h4 class="title">@service.Name</h4>
								<div class="price">
									<p class="amount">@service.Price<span>/ Per Visit</span></p>
								</div>
							</div>
							<!-- Table List -->
							<ul class="table-list">
								<li><i class="icofont icofont-ui-check"></i>Comprehensive consultation to assess your needs</li>
								<li><i class="icofont icofont-ui-check"></i>Personalized treatment plans tailored to your health</li>
								<li><i class="icofont icofont-ui-check"></i>Follow-up care to ensure optimal recovery and results</li>
								<li><i class="icofont icofont-ui-check"></i>Experienced and certified medical professionals</li>
								<li><i class="icofont icofont-ui-check"></i>Follow-up care to ensure optimal recovery and results</li>
							</ul>
							<div class="table-bottom">
								<a class="btn" href="#">Book Now</a>
							</div>
							<!-- Table Bottom -->
						</div>
					</div>
					<!-- End Single Table-->
					count++;
					if (count == 3)
					{
						break;
					}
				}
			}
			
		

		</div>
	</div>
</section>
<!--/ End Pricing Table -->
